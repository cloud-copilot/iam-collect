{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "CollectIAMData",
      "Effect": "Allow",
      "Action": [
        "account:ListRegions",
        "iam:GetAccountAuthorizationDetails",
        "lambda:ListFunctions",
        "lambda:ListTags",
        "lambda:GetPolicy",
        "s3:ListAllMyBuckets",
        "s3:GetBucketPolicy",
        "s3:GetBucketTagging",
        "s3:GetBucketPublicAccessBlock",
        "s3:GetEncryptionConfiguration",
        "kms:ListKeys",
        "kms:GetKeyPolicy",
        "kms:ListResourceTags"
      ],
      "Resource": "*"
    },
    {
      "Sid": "AssumeRoles",
      "Effect": "Allow",
      "Action": "sts:AssumeRole",
      "Resource": "*"
    }
  ]
}
